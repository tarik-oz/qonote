{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TokenSettings": {
    "Secret": "",
    "Issuer": "qonote.api.server",
    "Audience": "qonote.web.client",
    "TokenValidityInMinutes": 15,
    "RefreshTokenValidityInDays": 7
  },
  "GoogleSettings": {
    "ClientId": "",
    "ClientSecret": "",
    "RedirectUri": ""
  },
  "BlobStorage": {
    "ConnectionString": ""
  },
  "ConnectionStrings": {
    "QonoteDbConnection": ""
  },
  "EmailSettings": {
    "ConnectionString": "",
    "SenderAddress": ""
  },
  "Caching": {
    "Enabled": true,
    "Redis": {
      "ConnectionString": ""
    },
    "Sidebar": {
      "Enabled": true,
      "TtlSeconds": 600
    },
    "Me": {
      "Enabled": true,
      "TtlSeconds": 1200
    }
  },
  "Client": {
    "BaseUrl": ""
  },
  "YouTube": {
    "ApiKey": ""
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "ApplicationInsights": {
    "ConnectionString": ""
  }
}
